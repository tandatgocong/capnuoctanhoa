<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <!--
    Connection 
  -->
  <connectionStrings>
    <add name="CAPNUOCTANHOA.Properties.Settings.ConnectionString"
      connectionString="Data Source=.;Initial Catalog=CAPNUOCTANHOA;Persist Security Info=True;User ID=sa;Password=sa123"
      providerName="System.Data.SqlClient" />
    <add name="CAPNUOCTANHOA.Properties.Settings.AccessFile" connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=E:\QUANLYTHAYDHN.mdb" />
  </connectionStrings>
  <!--  
    End Connection
  -->
  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
